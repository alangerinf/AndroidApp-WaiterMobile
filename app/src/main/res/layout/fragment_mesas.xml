<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListMesasFragment">

<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/mesa_capafondo"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/mesa_lottie"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:alpha="0.5"
        android:visibility="visible"

        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_fileName="loco.json"
        app:lottie_imageAssetsFolder="assets"
        app:lottie_loop="true"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_clIzquierda"
        android:layout_width="32dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_clDerecha"
        android:layout_width="32dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_capaBlanca"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mesa_clDerecha"
        app:layout_constraintStart_toEndOf="@+id/mesa_clIzquierda"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_capafondoRV"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.8"
        android:background="@drawable/shape_bg_tunel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mesa_clDerecha"
        app:layout_constraintStart_toEndOf="@+id/mesa_clIzquierda"
        app:layout_constraintTop_toTopOf="@+id/mesa_headerbotton">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mesa_clDerecha"
        app:layout_constraintStart_toEndOf="@+id/mesa_clIzquierda"
        app:layout_constraintTop_toTopOf="@+id/mesa_headerbotton">

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/mesa_lottieBgTunel"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="centerCrop"

            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:lottie_autoPlay="true"
            app:lottie_fileName="bg_tunel.json"
            app:lottie_imageAssetsFolder="assets"
            app:lottie_loop="true"
            app:lottie_scale="0.1"
            tools:ignore="MissingConstraints" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/mesa_lottieBgTune2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="centerCrop"

            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mesa_lottieBgTunel"
            app:lottie_autoPlay="true"
            app:lottie_fileName="bg_tunel.json"
            app:lottie_imageAssetsFolder="assets"
            app:lottie_loop="true"
            app:lottie_scale="0.1"
            tools:ignore="MissingConstraints" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/mesa_lottieBgTune3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="centerCrop"

            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mesa_lottieBgTune2"
            app:lottie_autoPlay="true"
            app:lottie_fileName="bg_tunel.json"
            app:lottie_imageAssetsFolder="assets"
            app:lottie_loop="true"
            app:lottie_scale="0.1"
            tools:ignore="MissingConstraints" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/mesa_lottieBgTune4"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="centerCrop"

            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mesa_lottieBgTune3"
            app:lottie_autoPlay="true"
            app:lottie_fileName="bg_tunel.json"
            app:lottie_imageAssetsFolder="assets"
            app:lottie_loop="true"
            app:lottie_scale="0.1"
            tools:ignore="MissingConstraints" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/mesa_lottieBgTune5"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="centerCrop"

            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mesa_lottieBgTune4"
            app:lottie_autoPlay="true"
            app:lottie_fileName="bg_tunel.json"
            app:lottie_imageAssetsFolder="assets"
            app:lottie_loop="true"
            app:lottie_scale="0.1"
            tools:ignore="MissingConstraints" />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/mesa_lottieBgTune6"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scaleType="centerCrop"

            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mesa_lottieBgTune5"
            app:lottie_autoPlay="true"
            app:lottie_fileName="bg_tunel.json"
            app:lottie_imageAssetsFolder="assets"
            app:lottie_loop="true"
            app:lottie_scale="0.1"
            tools:ignore="MissingConstraints" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/mesa_rViewPallets"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:background="@null"
            android:visibility="invisible"
            app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:spanCount="2">

        </androidx.recyclerview.widget.RecyclerView>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_topeDerecho"
        android:layout_width="12dp"
        android:layout_height="match_parent"
  android:background="@drawable/shape_shadow_line_vertical"
      app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/mesa_constraintLayout"
        app:layout_constraintStart_toEndOf="@+id/mesa_constraintLayout"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_topeIzquierdo"
        android:layout_width="12dp"
        android:layout_height="match_parent"
        android:background="@drawable/shape_shadow_line_vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mesa_constraintLayout"
        app:layout_constraintStart_toStartOf="@+id/mesa_constraintLayout"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mesa_headerbotton"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@drawable/shape_header"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/mesa_gear"
        android:layout_width="52dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:scaleType="centerCrop"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/mesa_headerbotton"
        app:lottie_autoPlay="true"
        app:lottie_fileName="gear2.json"
        app:lottie_imageAssetsFolder="assets"
        app:lottie_loop="true"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="411dp" />}


</androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>